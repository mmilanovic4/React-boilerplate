{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating React applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} *.{js,json}",
		"build:dev": "webpack --progress",
		"build:prod": "webpack --env prod --progress",
		"build:stats": "webpack --env prod --progress --profile --json > stats.json",
		"start:dev": "webpack serve --content-base dist --host 0.0.0.0 --port 8000 --open --disable-host-check"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/react-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "^7.12.7",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"babel-loader": "^8.2.1",
		"copy-webpack-plugin": "^6.3.2",
		"css-loader": "^5.0.1",
		"mini-css-extract-plugin": "^1.3.1",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"prettier": "^2.2.0",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.0.3",
		"webpack": "^5.6.0",
		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"core-js": "^3.7.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"regenerator-runtime": "^0.13.5"
	}
}
