{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating React applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} *.{js,json}",
		"dev": "webpack --env dev --progress",
		"prod": "webpack --progress",
		"stats": "webpack --progress --profile --json > stats.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/react-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "7.21.4",
		"@babel/preset-react": "^7.18.6",
		"autoprefixer": "10.4.14",
		"babel-loader": "9.1.2",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "6.7.3",
		"css-minimizer-webpack-plugin": "5.0.0",
		"mini-css-extract-plugin": "2.7.5",
		"postcss": "8.4.21",
		"postcss-loader": "7.1.0",
		"prettier": "2.8.7",
		"style-loader": "3.3.2",
		"terser-webpack-plugin": "5.3.7",
		"webpack": "5.77.0",
		"webpack-cli": "5.0.1"
	},
	"dependencies": {
		"core-js": "3.29.1",
		"raf": "^3.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"regenerator-runtime": "^0.13.10"
	}
}
