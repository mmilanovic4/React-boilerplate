{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating React applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} *.{js,json}",
		"build:dev": "webpack --env dev --progress",
		"build:prod": "webpack --progress",
		"build:stats": "webpack --progress --profile --json > stats.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/react-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "^7.14.2",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/preset-env": "^7.14.2",
		"@babel/preset-react": "^7.13.13",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^8.1.1",
		"css-loader": "^5.2.4",
		"css-minimizer-webpack-plugin": "^3.0.0",
		"mini-css-extract-plugin": "^1.6.0",
		"prettier": "^2.3.0",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.1.2",
		"webpack": "^5.37.0",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"core-js": "^3.12.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"regenerator-runtime": "^0.13.5"
	}
}
