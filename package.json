{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating React applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} .prettierrc.json babel.config.js package.json webpack.config.js",
		"build:dev": "webpack --progress",
		"build:prod": "webpack --env.prod --progress",
		"build:stats": "webpack --env.prod --progress --profile --json > stats.json",
		"start:dev": "webpack-dev-server --content-base dist --host 0.0.0.0 --port 8000 --open --disable-host-check"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/react-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "^7.7.5",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-react": "^7.7.4",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.3.0",
		"file-loader": "^5.0.2",
		"prettier": "1.19.1",
		"style-loader": "^1.0.1",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0",
		"webpack-merge": "^4.2.2"
	},
	"dependencies": {
		"core-js": "^3.4.8",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"regenerator-runtime": "^0.13.3"
	}
}
