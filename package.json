{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating React applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} *.{js,json}",
		"dev": "webpack --env dev --progress",
		"prod": "webpack --progress",
		"stats": "webpack --progress --profile --json > stats.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/react-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "7.24.7",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "7.24.7",
		"@babel/preset-react": "7.24.7",
		"autoprefixer": "10.4.19",
		"babel-loader": "9.1.3",
		"copy-webpack-plugin": "12.0.2",
		"css-loader": "7.1.2",
		"css-minimizer-webpack-plugin": "7.0.0",
		"mini-css-extract-plugin": "2.9.0",
		"postcss": "8.4.38",
		"postcss-loader": "8.1.1",
		"prettier": "3.3.2",
		"style-loader": "4.0.0",
		"terser-webpack-plugin": "5.3.10",
		"webpack": "5.91.0",
		"webpack-cli": "5.1.4"
	},
	"dependencies": {
		"core-js": "3.37.1",
		"raf": "^3.4.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"regenerator-runtime": "0.14.1"
	}
}
